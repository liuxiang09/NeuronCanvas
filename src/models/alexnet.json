{
  "metadata": {
    "name": "alexnet",
    "displayName": "AlexNet",
    "description": "AlexNet 是由 Alex Krizhevsky 等人于 2012 年提出的深度卷积神经网络,在 ImageNet 竞赛中取得突破性成绩,标志着深度学习时代的开始。它首次证明了深度卷积网络在大规模图像识别任务中的强大能力。",
    "year": 2012,
    "authors": ["Alex Krizhevsky", "Ilya Sutskever", "Geoffrey Hinton"],
    "paper": "https://papers.nips.cc/paper/4824-imagenet-classification-with-deep-convolutional-neural-networks.pdf",
    "paperTitle": "ImageNet classification with deep convolutional neural networks",
    "citations": 149299,
    "tags": ["CNN", "图像分类", "经典", "ImageNet"],
    "category": "CNN"
  },
  "layers": [
    {
      "id": "input",
      "name": "输入层",
      "type": "input",
      "description": "接收原始的图像数据，每个图像为227x227像素，有3个颜色通道（RGB）。",
      "shape": [227, 227, 3]
    },
    {
      "id": "conv1",
      "name": "卷积层-1",
      "type": "conv2d",
      "description": "使用较大的卷积核快速提取图像的低级特征，如边缘、角落和颜色块。",
      "filters": 96,
      "kernelSize": [11, 11],
      "stride": [4, 4],
      "padding": [0, 0],
      "activation": "relu",
      "outputShape": [55, 55, 96]
    },
    {
      "id": "pool1",
      "name": "池化层-1",
      "type": "maxpool2d",
      "description": "对特征图进行下采样，减少数据维度，保留最显著的特征，并提供平移不变性。",
      "poolSize": [3, 3],
      "stride": [2, 2],
      "padding": [0, 0],
      "outputShape": [27, 27, 96]
    },
    {
      "id": "conv2",
      "name": "卷积层-2",
      "type": "conv2d",
      "description": "在第一层的基础上学习更复杂、更抽象的特征组合。",
      "filters": 256,
      "kernelSize": [5, 5],
      "stride": [1, 1],
      "padding": [2, 2],
      "activation": "relu",
      "outputShape": [27, 27, 256]
    },
    {
      "id": "pool2",
      "name": "池化层-2",
      "type": "maxpool2d",
      "description": "进一步缩小特征图的尺寸，减少计算量，并增强特征的鲁棒性。",
      "poolSize": [3, 3],
      "stride": [2, 2],
      "padding": [0, 0],
      "outputShape": [13, 13, 256]
    },
    {
      "id": "conv3",
      "name": "卷积层-3",
      "type": "conv2d",
      "description": "连续的卷积层之一，使用更小的卷积核来加深网络，学习更深层次的特征。",
      "filters": 384,
      "kernelSize": [3, 3],
      "stride": [1, 1],
      "padding": [1, 1],
      "activation": "relu",
      "outputShape": [13, 13, 384]
    },
    {
      "id": "conv4",
      "name": "卷积层-4",
      "type": "conv2d",
      "description": "与第三层结构类似，继续提取更复杂的特征表示。",
      "filters": 384,
      "kernelSize": [3, 3],
      "stride": [1, 1],
      "padding": [1, 1],
      "activation": "relu",
      "outputShape": [13, 13, 384]
    },
    {
      "id": "conv5",
      "name": "卷积层-5",
      "type": "conv2d",
      "description": "最后一个卷积层，为即将到来的全连接层准备最终的特征图。",
      "filters": 256,
      "kernelSize": [3, 3],
      "stride": [1, 1],
      "padding": [1, 1],
      "activation": "relu",
      "outputShape": [13, 13, 256]
    },
    {
      "id": "pool3",
      "name": "池化层-3",
      "type": "maxpool2d",
      "description": "最后一次下采样，将特征图压缩到适合全连接层处理的大小。",
      "poolSize": [3, 3],
      "stride": [2, 2],
      "padding": [0, 0],
      "outputShape": [6, 6, 256]
    },
    {
      "id": "flatten",
      "name": "展平层",
      "type": "flatten",
      "description": "将卷积层提取的二维特征图“压平”成一维向量",
      "outputShape": [9216]
    },
    {
      "id": "fc1",
      "name": "全连接层-1",
      "type": "dense",
      "description": "使用全连接层进行高级特征的组合与分类。",
      "activation": "relu",
      "outputShape": [4096]
    },
    {
      "id": "dropout1",
      "name": "Dropout层-1",
      "type": "dropout",
      "description": "在训练期间随机“丢弃”一部分神经元，防止神经元之间过度依赖，从而减轻过拟合。",
      "rate": 0.5,
      "outputShape": [4096]
    },
    {
      "id": "fc2",
      "name": "全连接层-2",
      "type": "dense",
      "description": "进一步整合特征，增强模型的非线性表达能力。",
      "activation": "relu",
      "outputShape": [4096]
    },
    {
      "id": "dropout2",
      "name": "Dropout层-2",
      "type": "dropout",
      "description": "再次使用Dropout技术以增强模型的泛化能力。",
      "rate": 0.5,
      "outputShape": [4096]
    },
    {
      "id": "output",
      "name": "输出层",
      "type": "output",
      "description": "最终的分类层，将特征映射到1000个类别（针对ImageNet数据集），Softmax函数输出每个类别的概率。",
      "activation": "softmax",
      "outputShape": [1000]
    }
  ],
  "edges": [
    {
      "id": "e1",
      "source": "input",
      "target": "conv1",
      "type": "normal"
    },
    {
      "id": "e2",
      "source": "conv1",
      "target": "pool1",
      "type": "normal"
    },
    {
      "id": "e3",
      "source": "pool1",
      "target": "conv2",
      "type": "normal"
    },
    {
      "id": "e4",
      "source": "conv2",
      "target": "pool2",
      "type": "normal"
    },
    {
      "id": "e5",
      "source": "pool2",
      "target": "conv3",
      "type": "normal"
    },
    {
      "id": "e6",
      "source": "conv3",
      "target": "conv4",
      "type": "normal"
    },
    {
      "id": "e7",
      "source": "conv4",
      "target": "conv5",
      "type": "normal"
    },
    {
      "id": "e8",
      "source": "conv5",
      "target": "pool3",
      "type": "normal"
    },
    {
      "id": "e9",
      "source": "pool3",
      "target": "flatten",
      "type": "normal"
    },
    {
      "id": "e10",
      "source": "flatten",
      "target": "fc1",
      "type": "normal"
    },
    {
      "id": "e11",
      "source": "fc1",
      "target": "dropout1",
      "type": "normal"
    },
    {
      "id": "e12",
      "source": "dropout1",
      "target": "fc2",
      "type": "normal"
    },
    {
      "id": "e13",
      "source": "fc2",
      "target": "dropout2",
      "type": "normal"
    },
    {
      "id": "e14",
      "source": "dropout2",
      "target": "output",
      "type": "normal"
    }
  ]
}
