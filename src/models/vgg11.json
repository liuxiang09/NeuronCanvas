      "outputShape": [64, 1000]
    },
    {
      "id": "output",
      "name": "输出层",
      "type": "linear",
      "units": 1000,
      "outputType": "classification"
    },
    {
      "id": "output_act",
      "name": "Softmax 激活",
      "type": "softmax",
      "activation": "softmax"
    }
