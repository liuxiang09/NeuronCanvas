{
  "metadata": {
    "name": "resnet18",
    "displayName": "ResNet-18(未验证)",
    "description": "ResNet-18 是 ResNet 系列中最轻量的变体，包含 18 层网络。使用基本残差块（Basic Block）而非瓶颈结构，适合资源受限场景，同时保留了残差连接的核心优势。",
    "year": 2015,
    "authors": ["Kaiming He", "Xiangyu Zhang", "Shaoqing Ren", "Jian Sun"],
    "paper": "https://arxiv.org/abs/1512.03385",
    "paperTitle": "Deep residual learning for image recognition",
    "citations": 292879,
    "tags": ["CNN", "残差网络", "图像分类", "深度学习"],
    "category": "CNN"
  },
  "layers": [
    {
      "id": "input",
      "name": "输入层",
      "type": "input",
      "description": "224×224 RGB 图像输入",
      "shape": [224, 224, 3]
    },
    {
      "id": "conv1",
      "name": "Conv1",
      "type": "conv2d",
      "description": "初始卷积层",
      "filters": 64,
      "kernelSize": [7, 7],
      "stride": [2, 2],
      "padding": [1, 1],
      "activation": "relu",
      "outputShape": [112, 112, 64]
    },
    {
      "id": "pool1",
      "name": "MaxPool",
      "type": "maxpool2d",
      "description": "最大池化",
      "poolSize": [3, 3],
      "stride": [2, 2],
      "outputShape": [56, 56, 64]
    },
    {
      "id": "res2a",
      "name": "Residual Block 2a",
      "type": "stem",
      "color": "emerald",
      "inputShape": [56, 56, 64],
      "outputShape": [56, 56, 64],
      "steps": [
        {
          "id": "res2a_3x3_1",
          "name": "3×3 Conv 1",
          "type": "conv2d",
          "filters": 64,
          "kernelSize": [3, 3],
          "stride": [1, 1],
          "padding": [1, 1],
          "activation": "relu",
          "outputShape": [56, 56, 64]
        },
        {
          "id": "res2a_3x3_2",
          "name": "3×3 Conv 2",
          "type": "conv2d",
          "filters": 64,
          "kernelSize": [3, 3],
          "stride": [1, 1],
          "padding": [1, 1],
          "activation": "relu",
          "outputShape": [56, 56, 64]
        }
      ]
    },
    {
      "id": "add2a",
      "name": "Add 2a",
      "type": "add",
      "description": "残差相加",
      "outputShape": [56, 56, 64]
    },
    {
      "id": "res2b",
      "name": "Residual Block 2b",
      "type": "stem",
      "color": "emerald",
      "inputShape": [56, 56, 64],
      "outputShape": [56, 56, 64],
      "steps": [
        {
          "id": "res2b_3x3_1",
          "name": "3×3 Conv 1",
          "type": "conv2d",
          "filters": 64,
          "kernelSize": [3, 3],
          "stride": [1, 1],
          "padding": [1, 1],
          "activation": "relu",
          "outputShape": [56, 56, 64]
        },
        {
          "id": "res2b_3x3_2",
          "name": "3×3 Conv 2",
          "type": "conv2d",
          "filters": 64,
          "kernelSize": [3, 3],
          "stride": [1, 1],
          "padding": [1, 1],
          "activation": "relu",
          "outputShape": [56, 56, 64]
        }
      ]
    },
    {
      "id": "add2b",
      "name": "Add 2b",
      "type": "add",
      "description": "残差相加",
      "outputShape": [56, 56, 64]
    },
    {
      "id": "res3a",
      "name": "Residual Block 3a",
      "type": "stem",
      "color": "emerald",
      "inputShape": [56, 56, 64],
      "outputShape": [28, 28, 128],
      "steps": [
        {
          "id": "res3a_3x3_1",
          "name": "3×3 Conv 1",
          "type": "conv2d",
          "filters": 128,
          "kernelSize": [3, 3],
          "stride": [2, 2],
          "padding": [1, 1],
          "activation": "relu",
          "outputShape": [28, 28, 128]
        },
        {
          "id": "res3a_3x3_2",
          "name": "3×3 Conv 2",
          "type": "conv2d",
          "filters": 128,
          "kernelSize": [3, 3],
          "stride": [1, 1],
          "padding": [1, 1],
          "activation": "relu",
          "outputShape": [28, 28, 128]
        }
      ]
    },
    {
      "id": "proj3a",
      "name": "Projection 3a",
      "type": "conv2d",
      "description": "投影层",
      "filters": 128,
      "kernelSize": [1, 1],
      "stride": [2, 2],
      "padding": [1, 1],
      "outputShape": [28, 28, 128]
    },
    {
      "id": "add3a",
      "name": "Add 3a",
      "type": "add",
      "description": "残差相加",
      "outputShape": [28, 28, 128]
    },
    {
      "id": "pool_final",
      "name": "Global Avg Pool",
      "type": "maxpool2d",
      "description": "全局平均池化",
      "poolSize": [7, 7],
      "stride": [1, 1],
      "outputShape": [1, 1, 128]
    },
    {
      "id": "flatten",
      "name": "Flatten",
      "type": "flatten",
      "description": "展平为一维向量",
      "outputShape": [128]
    },
    {
      "id": "fc",
      "name": "全连接层",
      "type": "dense",
      "description": "最终分类层",
      "units": 1000,
      "activation": "relu",
      "outputShape": [1000]
    },
    {
      "id": "output",
      "name": "输出层",
      "type": "output",
      "description": "1000 类别输出",
      "units": 1000,
      "activation": "softmax",
      "outputType": "classification"
    }
  ],
  "edges": [
    {
      "id": "e1",
      "source": "input",
      "target": "conv1",
      "type": "normal"
    },
    {
      "id": "e2",
      "source": "conv1",
      "target": "pool1",
      "type": "normal"
    },
    {
      "id": "e3",
      "source": "pool1",
      "target": "res2a",
      "type": "normal"
    },
    {
      "id": "e4",
      "source": "res2a",
      "target": "add2a",
      "type": "normal"
    },
    {
      "id": "e5",
      "source": "pool1",
      "target": "add2a",
      "type": "residual"
    },
    {
      "id": "e6",
      "source": "add2a",
      "target": "res2b",
      "type": "normal"
    },
    {
      "id": "e7",
      "source": "res2b",
      "target": "add2b",
      "type": "normal"
    },
    {
      "id": "e8",
      "source": "add2a",
      "target": "add2b",
      "type": "residual"
    },
    {
      "id": "e9",
      "source": "add2b",
      "target": "res3a",
      "type": "normal"
    },
    {
      "id": "e10",
      "source": "res3a",
      "target": "add3a",
      "type": "normal"
    },
    {
      "id": "e11",
      "source": "add2b",
      "target": "proj3a",
      "type": "residual"
    },
    {
      "id": "e12",
      "source": "proj3a",
      "target": "add3a",
      "type": "residual"
    },
    {
      "id": "e13",
      "source": "add3a",
      "target": "pool_final",
      "type": "normal"
    },
    {
      "id": "e14",
      "source": "pool_final",
      "target": "flatten",
      "type": "normal"
    },
    {
      "id": "e15",
      "source": "flatten",
      "target": "fc",
      "type": "normal"
    },
    {
      "id": "e16",
      "source": "fc",
      "target": "output",
      "type": "normal"
    }
  ]
}
